
<div id="master-map-container">
    <div id="counties" class="tab-container" style="<%= 'display:none;' if current_page.path == 'recruits.html' %>">
      <div class="legend">
        <span class="title">More Recruits &rarr;</span>
        <ul class="ramp">
          <li style="background-color:#191919"></li>
          <li style="background-color:#0e270e"></li>
          <li style="background-color:#0d3b0d"></li>
          <li style="background-color:#0a550a"></li>
          <li style="background-color:#0c690b"></li>
          <li style="background-color:#128612"></li>
        </ul>
        <span class="title">Recruit level</span>
        <ul class="recruit-ramp">
          <li class="star5"><span></span> 5&#9733;</li>
          <li class="star4"><span></span> 4&#9733;</li>
          <li class="star3"><span></span> 3&#9733;</li>
          <li class="star2"><span></span> 2&#9733;</li>
        </ul>
        <span class="title">
          <span class="school">&star;</span>School
        </span>
      </div>

      <div class="legend county-info js-county-info">
        <div class="county-info"></div>
      </div>
      <div id="county-map" class="map">
        <span class="js-loading loading">Loading...</span>
      </div>
    </div>
    <div id="recruits" class="tab-container" style="<%= 'display:none;' if current_page.path != 'recruits.html' %>">
      <div id="recruit-map" class="map"></div>
    </div>
</div>
